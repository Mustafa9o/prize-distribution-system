{
  "version": 2,
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.0",
      "includeFiles": "logo.png"
    }
  },
  "routes": [
    { "handle": "filesystem" },
    { "src": "/", "dest": "/api/index.php" },
    { "src": "/(.*)", "dest": "/api/index.php" }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html; charset=utf-8"
        }
      ]
    }
  ]
}
